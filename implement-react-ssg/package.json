{
  "name": "sample-webpack",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rimraf dist && node script/server.dev.js",
    "build:prod": "concurrently \"pnpm run build:client:prod\" \"pnpm run build:server:prod\" ",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config config/prod.client.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack --config config/prod.server.js",
    "build:ssg": "rimraf dist && pnpm run build:prod && node script/generate.prod.js",
    "preview:ssg": "pnpm run build:ssg && node script/preview.ssg.js"
  },
  "keywords": [],
