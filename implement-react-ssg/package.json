    "@types/node": "^20.14.9",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.1.3",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "express": "^4.19.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.6.0",
