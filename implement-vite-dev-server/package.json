{
  "name": "vite-dev-server-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:build": "nodemon --config devServer/config/nodemon.dev.build.json",
    "dev:start": "nodemon --config devServer/config/nodemon.dev.start.json",
    "prod:build": "nodemon --config prodBuilder/config/nodemon.prod.build.json",
    "prod:start": "nodemon --config prodBuilder/config/nodemon.prod.start.json",
    "prod:roll": "nodemon --config rollupBuilder/nodemon.rollup.build.json",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/resolve": "^1.20.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "es-module-lexer": "^1.4.1",
    "esbuild": "^0.19.12",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.18.2",
    "magic-string": "^0.30.5",
    "nodemon": "^3.0.3",
    "picocolors": "^1.0.0",
    "resolve": "^1.22.8",
    "rollup": "^4.9.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=8.7.6"
  }
}
